{
    "extends": "tslint:latest",
    "rules": {
      "semicolon": [
        true,
        "never"
      ],
      "quotemark": [
        true,
        "single"
      ],
      "trailing-comma": [
        true, 
        {
          "multiline": "never",
          "singleline": "never"
        }
      ],
      "triple-equals": [
        true,
        "allow-null-check",
        "allow-undefined-check"
      ],
      "no-console": [
        false
      ],
      "no-string-literal": [
        false
      ],
      "prefer-conditional-expression": [
        false
      ],
      "no-var-requires": false,
      "object-literal-sort-keys": false,
      "interface-name": [true, "never-prefix"]
    }
  }